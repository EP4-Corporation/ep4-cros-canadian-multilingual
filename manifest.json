{

  "name": "EP4 - Canadian Multilingual Keyboard Fix",
  "description": "Fixes the AltGr behaviour, enabling you to type special characters",
  "version": "1.0.1",
  "manifest_version": 2,
  "minimum_chrome_version": "52",
  "icons": {
     "16": "icons/icon16.png",
     "32": "icons/icon32.png",
     "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content_script.js"],
      "all_frames": true,
      "run_at": "document_start"
    }
  ]
}
